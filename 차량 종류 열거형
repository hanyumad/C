#include <stdio.h>

enum Car {SEDAN = 1, SUV, TRUCK, VAN};

int main() {
    
    int input;

    printf("Car Type: 1. SEDAN  2. SUV  3. TRUCK  4. VAN\n");
    scanf("%d", &input);

    switch (input) {
        case 1:
            printf("Car type is SEDAN\n");
            break;
        case 2:
            printf("Car type is SUV\n");
            break;
        case 3:
            printf("Car type is TRUCK\n");
            break;
        case 4:
            printf("Car type is VAN\n");
            break;
        default:
            printf("Invalid input\n");
    }

    return 0;
}
